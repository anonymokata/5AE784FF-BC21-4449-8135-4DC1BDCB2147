#test roman2int_test
	ck_assert_int_eq (roman2int ("I"), 1);
	ck_assert_int_eq (roman2int ("II"), 2);
	ck_assert_int_eq (roman2int ("III"), 3);
	ck_assert_int_eq (roman2int ("IV"), 4);
	ck_assert_int_eq (roman2int ("V"), 5);
